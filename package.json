  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/nodemon --exec ./node_modules/.bin/ts-node -- --fast --inspect -r tsconfig-paths/register src/ts/server.ts",
    "create_migration": "cp src/migrations/.base.ts src/migrations/`date +\"%Y%m%d%H%M%S\"`-migration.ts",
    "build_migrations": "./node_modules/.bin/tsc -p src/migrations/",
    "migrate": "./node_modules/.bin/sequelize db:migrate --config config/sequelize.js --migrations-path build/migrations/",
    "reset_migrations": "./node_modules/.bin/sequelize db:migrate:undo:all --config config/sequelize.js --migrations-path build/migrations/",
    "reset_last_migration": "./node_modules/.bin/sequelize db:migrate:undo --config config/sequelize.js --migrations-path build/migrations/",
    "sql_init": "./node_modules/.bin/sequelize init"
  },
