{
  "name": "saas-api",
  "version": "1.0.0",
  "description": "SaaS API",
  "main": "src/ts/server.ts",
  "dependencies": {
    "aws-sdk": "^2.78.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.15.2",
    "config": "^1.24.0",
    "continuation-local-storage": "^3.2.0",
    "cookie-parser": "^1.4.3",
    "dataloader-sequelize": "^1.5.2",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.2.4",
    "express": "^4.16.3",
    "express-graphql": "^0.6.6",
    "express-session": "^1.14.2",
    "graphql": "^0.13.2",
    "graphql-relay": "^0.5.5",
    "graphql-type-json": "^0.1.4",
    "immutable": "^3.8.1",
    "jsonwebtoken": "^7.2.1",
    "libpq": "^1.8.8",
    "lodash": "^4.17.4",
    "node-gyp": "^3.8.0",
    "object-hash": "^1.1.8",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "pg-native": "^3.0.0",
    "redis": "^2.7.1",
    "sequelize": "^4.1.0",
    "sequelize-cli": "^2.5.1",
    "socketio": "^1.0.0",
    "uuid": "^3.0.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/bluebird": "^3.5.8",
    "@types/body-parser": "0.0.33",
    "@types/continuation-local-storage": "^3.2.1",
    "@types/cookie-parser": "^1.3.30",
    "@types/express": "^4.16.0",
    "@types/express-graphql": "^0.0.33",
    "@types/express-session": "0.0.32",
    "@types/graphql-relay": "^0.4.5",
    "@types/jsonwebtoken": "^7.2.1",
    "@types/redis": "^0.12.36",
    "@types/socket.io": "^1.4.28",
    "@types/uuid": "^2.0.29",
    "@types/winston": "^2.2.0",
    "config": "^1.24.0",
    "nodemon": "^1.11.0",
    "ts-node": "^7.0.1",
    "tslint": "^4.3.1",
    "typescript": "^3.0.1"
  },
  "scripts": {
    "build": "rm -rf build/app && NODE_ENV=production ./node_modules/.bin/tsc",
    "start": "NODE_ENV=development ./node_modules/.bin/nodemon --exec ./node_modules/.bin/ts-node -- -T src/ts/server.ts",
    "create-migration": "cp src/migrations/.base.ts src/migrations/`date +\"%Y%m%d%H%M%S\"`-migration.ts",
    "build-migrations": "rm -rf build/migrations && tsc --build tsconfig.migrations.json",
    "migrate": "./node_modules/.bin/sequelize db:migrate --config config/sequelize.js --migrations-path build/migrations/",
    "reset-migrations": "./node_modules/.bin/sequelize db:migrate:undo:all --config config/sequelize.js --migrations-path build/migrations/",
    "reset-last-migration": "./node_modules/.bin/sequelize db:migrate:undo --config config/sequelize.js --migrations-path build/migrations/",
    "sql-init": "./node_modules/.bin/sequelize init"
  },
  "author": "tills13",
